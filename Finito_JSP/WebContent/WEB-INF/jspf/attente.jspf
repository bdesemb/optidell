<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>

<c:choose>
	<c:when test='${partie.etat=="fini"}'>
		<p> Partie annulée ! </p>
	</c:when>
	<c:when test='${partie.etat!="en_cours"}'>
		<p> En attente des autres joueurs ... </p>
	</c:when>
	<c:otherwise>
		<jsp:forward page="jouerPartie"></jsp:forward>
	</c:otherwise>
</c:choose>